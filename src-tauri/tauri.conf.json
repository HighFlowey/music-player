{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "music-player",
    "version": "0.0.2"
  },
  "tauri": {
    "systemTray": {
      "iconPath": "icons/icon.ico"
    },
    "allowlist": {
      "all": false,
      "dialog": {
        "all": false,
        "ask": false,
        "confirm": false,
        "message": false,
        "open": true,
        "save": false
      },
      "shell": {
        "all": false,
        "open": true
      },
      "protocol": {
        "all": true,
        "asset": true,
        "assetScope": [
          "**"
        ]
      }
    },
    "windows": [
      {
        "title": "music-player",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": "default-src 'self'; img-src 'self' data:; media-src 'self' asset: https://asset.localhost;"
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.highflowey.music-player",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}